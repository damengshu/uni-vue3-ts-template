{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "%index.title%"
      }
    },
    {
      "path": "pages/list/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/formula/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/profile/index",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "tabBar": {
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/tabBar/index.png",
        "selectedIconPath": "static/tabBar/index-full.png",
        "text": "%tabbar.home%"
      },
      {
        "pagePath": "pages/list/index",
        "iconPath": "static/tabBar/list.png",
        "selectedIconPath": "static/tabBar/list-full.png",
        "text": "%tabbar.list%"
      },
      {
        "pagePath": "pages/formula/index",
        "iconPath": "static/tabBar/details.png",
        "selectedIconPath": "static/tabBar/details-full.png",
        "text": "%tabbar.formula%"
      },
      {
        "pagePath": "pages/profile/index",
        "iconPath": "static/tabBar/profile.png",
        "selectedIconPath": "static/tabBar/profile-full.png",
        "text": "%tabbar.profile%"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "easycom": {
    "custom": {
      "^u-(.*)": "vk-uview-ui/components/u-$1/u-$1.vue"
    }
  }
}
